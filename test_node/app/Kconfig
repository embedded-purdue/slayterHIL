menu "Zephyr"
source "Kconfig.zephyr"
endmenu

module = APP
module-str = APP

config I2C_0_SCL_GPIO
	int "I2C0 SCL GPIO pin"
	default 9

config I2C_0_SDA_GPIO
	int "I2C0 SDA GPIO pin"
	default 8
